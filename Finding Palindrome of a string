/* package whatever; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;


/* Name of the class has to be "Main" only if the class is public. */
class Ideone
{
	public static void main (String[] args) throws java.lang.Exception
	{
	String str="Malam";
	StringBuffer strBldr=new StringBuffer(str);
	strBldr.reverse();
	
	System.out.println(" Result of the palindrome--"+strBldr.toString().equals(str));
	
	}
}
